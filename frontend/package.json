    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autoprefixer": "^9",
    "axios": "^0.21.1",
    "color": "^3.1.3",
    "date-fns": "^2.16.1",
    "grommet": "^2.16.2",
    "immer": "^8.0.1",
