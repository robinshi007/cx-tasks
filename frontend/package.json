    "craco-less": "^1.17.1",
    "postcss-cli": "^8.3.1",
    "twin.macro": "^2.1.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpackbar": "^5.0.0-3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
